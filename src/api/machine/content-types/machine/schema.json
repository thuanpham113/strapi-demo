{
  "kind": "collectionType",
  "collectionName": "machines",
  "info": {
    "singularName": "machine",
    "pluralName": "machines",
    "displayName": "Machine",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "tel": {
      "type": "string",
      "unique": true,
      "regex": "^(\\+84|0|1)\\d{9,10}$",
      "required": true
    },
    "address": {
      "type": "text"
    },
    "timeOut": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "notion": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "idMachine": {
      "type": "string",
      "required": true
    }
  }
}
